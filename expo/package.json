{
  "name": "expo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "restart:ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 pnpm install && cd ios && pod install && cd .. && pnpm prebuild:ios && pnpm start",
    "restart:android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 pnpm install && pnpm prebuild:android && pnpm run:android",
    "start": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start --dev-client",
    "android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:android",
    "ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:ios",
    "eject": "expo eject",
    "prebuild": "expo prebuild",
    "prebuild:ios": "expo prebuild --platform ios",
    "prebuild:android": "expo prebuild --platform android",
    "clean": "rm -rf .expo .turbo node_modules ios android dist build",
    "run:android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:android",
    "run:ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:ios",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/native": "^7.0.14",
    "expo": "catalog:",
    "expo-font": "~13.0.3",
    "expo-linking": "~7.0.4",
    "expo-router": "~4.0.16",
    "expo-splash-screen": "~0.29.20",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.7",
    "expo-web-browser": "~14.0.2",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-native": "catalog:",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "expo-constants": "~17.0.4",
    "react-native-gesture-handler": "~2.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.3",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.3.3",
    "@rnx-kit/metro-config": "^1.3.15",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.36"
  },
  "private": true
}