{
  "name": "porto-expo",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "restart:ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 pnpm install && cd ios && pod install && cd .. && pnpm prebuild:ios && pnpm start",
    "restart:android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 pnpm install && pnpm prebuild:android && pnpm run:android",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "eject": "expo eject",
    "prebuild": "expo prebuild",
    "prebuild:ios": "expo prebuild --platform ios",
    "prebuild:android": "expo prebuild --platform android",
    "clean": "rm -rf .expo .turbo node_modules ios android dist build",
    "run:android": "expo run:android",
    "run:ios": "expo run:ios",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios"
  },
  "dependencies": {
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@rnx-kit/metro-config": "^1.3.15",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.36",
    "expo": "^52.0.0",
    "expo-constants": "~17.0.3",
    "expo-splash-screen": "~0.29.18",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-mmkv": "^3.2.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.26.0",
    "@expo/cli": "^0.22.7",
    "@expo/metro-runtime": "~4.0.0",
    "@expo/webpack-config": "~19.0.1",
    "@react-native/metro-config": "^0.76.5",
    "@types/react": "catalog:",
    "@types/react-native": "catalog:",
    "expo-cli": "^6.3.10",
    "typescript": "catalog:"
  },
  "private": true
}
