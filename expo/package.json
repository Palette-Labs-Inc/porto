{
  "name": "porto-expo",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "restart:ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 pnpm install && cd ios && pod install && cd .. && pnpm prebuild:ios && pnpm start",
    "restart:android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 pnpm install && pnpm prebuild:android && pnpm run:android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "eject": "expo eject",
    "prebuild": "expo prebuild",
    "start": "expo start",
    "prebuild:ios": "expo prebuild --platform ios",
    "prebuild:android": "expo prebuild --platform android",
    "clean": "rm -rf .expo node_modules ios android dist build",
    "run:android": "EXPO_DEBUG=1 USE_PORTO_SOURCE=true expo run:android",
    "run:ios": "EXPO_DEBUG=1 USE_PORTO_SOURCE=true expo run:ios",
    "build:android": "EXPO_DEBUG=1 USE_PORTO_SOURCE=true expo build:android",
    "build:ios": "EXPO_DEBUG=1 expo build:ios",
    "web": "expo start --web",
    "ios:debug": "EXPO_DEBUG=1 USE_PORTO_SOURCE=true EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:ios",
    "android:debug": "EXPO_DEBUG=1 USE_PORTO_SOURCE=true EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:android",
    "studio:android": "pnpm prebuild:android && open -a /Applications/Android\\ Studio.app ./android"
  },
  "dependencies": {
    "@peculiar/webcrypto": "^1.5.0",
    "@porto/expo-p256": "workspace:*",
    "@porto/expo-webauthn": "workspace:*",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@rnx-kit/metro-config": "^1.3.15",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.36",
    "buffer": "^6.0.3",
    "expo": "catalog:",
    "expo-build-properties": "^0.13.2",
    "expo-crypto": "^14.0.2",
    "expo-linking": "^7.0.4",
    "expo-modules-autolinking": "^2.0.7",
    "expo-modules-core": "^2.1.4",
    "expo-router": "^4.0.16",
    "expo-splash-screen": "^0.29.21",
    "expo-status-bar": "~2.0.0",
    "ox": "catalog:",
    "porto": "workspace:*",
    "react": "^18.3.1",
    "react-native": "^0.76.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-mmkv": "^3.2.0",
    "react-native-nitro-modules": "^0.21.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.26.0",
    "@expo/metro-runtime": "~4.0.0",
    "@react-native/metro-config": "^0.76.5",
    "@types/react": "catalog:",
    "typescript": "catalog:"
  },
  "private": true
}
