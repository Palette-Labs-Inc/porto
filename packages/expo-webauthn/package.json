{
  "name": "@porto/expo-webauthn",
  "version": "0.0.1",
  "description": "Native Expo module provideing platform-specific implementations for createFn and getFn methods of the Ox WebAuthnP256 module.",
  "summary": "Native WebAuthnP256 implementation",
  "source": {
    "git": "https://github.com/Palette-Labs-Inc/porto.git"
  },
  "type": "module",
  "main": "./build/WebAuthN.js",
  "types": "./build/WebAuthN.d.ts",
  "scripts": {
    "build": "npm_config_user_agent=yarn expo-module build",
    "clean": "npm_config_user_agent=yarn expo-module clean",
    "lint": "npm_config_user_agent=yarn expo-module lint",
    "test": "npm_config_user_agent=yarn expo-module test",
    "prepare": "npm_config_user_agent=yarn expo-module prepare",
    "prepublishOnly": "npm_config_user_agent=yarn expo-module prepublishOnly",
    "postinstall": "pnpm build"
  },
  "keywords": ["react-native", "expo", "passkeys"],
  "author": "mike perhats",
  "license": "MIT",
  "homepage": "https://github.com/mperhats",
  "dependencies": {
    "ox": "catalog:"
  },
  "devDependencies": {
    "expo-module-scripts": "catalog:"
  },
  "peerDependencies": {
    "expo": "catalog:"
  },
  "jest": {
    "preset": "expo-module-scripts"
  }
}
