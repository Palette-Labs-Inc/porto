{
  "name": "porto",
  "description": "Next-gen Account for Ethereum",
  "version": "0.0.4",
  "type": "module",
  "exports": {
    ".": {
      "types": "./_dist/index.d.ts",
      "import": {
        "default": "./_dist/index.js"
      }
    },
    "./native": {
      "types": "./_dist/index.native.d.ts",
      "react-native": "./_dist/index.native.js"
    }
